{
  "name": "react-tiny-transition",
  "version": "2.1.2",
  "description":
    "A small component that adds class names on mount/unmount for you to add css transitions to",
  "main": "lib/index.js",
  "files": ["lib", "LICENSE", "README.md"],
  "scripts": {
    "start": "rm -fr lib/ && babel src --out-dir lib --ignore='__tests__'",
    "test": "jest --verbose",
    "pub": "npm test && npm start && npm publish",
    "dev": "webpack-dev-server --content-base dist/ --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/asbjornh/react-tiny-transition.git"
  },
  "keywords": ["react", "css-transition", "animation", "mount", "unmount"],
  "contributors": [
    {
      "name": "Asbjorn Hegdahl",
      "url": "https://github.com/asbjornh/"
    }
  ],
  "author": "Asbjorn Hegdahl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asbjornh/react-tiny-transition/issues"
  },
  "homepage": "https://github.com/asbjornh/react-tiny-transition#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^4.12.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-react": "^7.5.1",
    "expose-loader": "^0.7.4",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest-cli": "^21.2.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^16.2.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "jest": {
